{
  "name": "fetch-skill",
  "version": "1.0.0",
  "description": "Install agent skills onto coding agents (OpenCode, Claude Code, Codex, Cursor)",
  "type": "module",
  "bin": {
    "fetch-skill": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean",
    "dev": "tsx src/index.ts",
    "lint": "biome lint --write .",
    "format": "biome format --write .",
    "check": "biome check --write .",
    "prepublishOnly": "npm run build",
    "update-readme": "tsx scripts/update-readme.ts"
  },
  "keywords": [
    "cli",
    "skills",
    "opencode",
    "claude-code",
    "codex",
    "cursor",
    "amp",
    "antigravity",
    "roo-code",
    "ai-agents"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shaneholloman/fetch-skill.git"
  },
  "homepage": "https://github.com/shaneholloman/fetch-skill#readme",
  "bugs": {
    "url": "https://github.com/shaneholloman/fetch-skill/issues"
  },
  "author": "Shane Holloman",
  "license": "MIT",
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "gray-matter": "^4.0.3",
    "simple-git": "^3.30.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@types/node": "^25.0.9",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=22"
  }
}
